<h1>MultyTix</h1>



<%= form_tag do %>
  <%= label_tag(:my_name, "My name is:") %>
  <%= text_field_tag(:my_name) %>

  <%= label_tag(:my_phone, "Phone #:") %>
  <%= text_field_tag(:my_phone) %>

  <%= submit_tag("Process") %>
<% end %>




<%= select_tag(:city_id, options_for_select([['$1', 1], ['$5', 2],['$10', 3],['$20', 4]])) %>

